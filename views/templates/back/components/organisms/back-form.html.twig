{{ form_start(form) }}
  <div class="card">
    {% include '@Modules/kjmodulebedrock/views/templates/back/components/atoms/card-header.html.twig'
      with {'header': 'Card header' |trans({}, 'Modules.Kjmodulebedrock.Admin')} %}
    <div class="card-block row">
      <div class="card-text">
        {{ form_widget(form) }}
      </div>
    </div>
    <div class="card-footer">
      <div class="d-flex justify-content-end">
        <button class="btn btn-primary float-right" id="save-button">
          {{ 'Save'|trans({}, 'Admin.Actions') }}
        </button>
      </div>
    </div>
  </div>
{{ form_end(form) }}