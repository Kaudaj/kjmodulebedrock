{
    "name": "kaudaj/kjmodulebedrock",
    "description": "Module Bedrock",
    "type": "prestashop-module",
    "license": "AFL-3.0",
    "authors": [
        {
            "name": "Kaudaj",
            "email": "info@kaudaj.com",
            "homepage": "https://kaudaj.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "require-dev": {
        "prestashop/php-dev-tools": "^4.1",
        "phpstan/phpstan": "^0.12.90",
        "prestashop/autoindex": "^2.0",
        "prestashop/header-stamp": "^2.0",
        "phpro/grumphp-shim": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Kaudaj\\Module\\ModuleBedrock\\": "src/"
        }
    },
    "config": {
        "prepend-autoloader": false
    },
    "extra": {
        "grumphp": {
            "config-default-path": "tests/php/grumphp.yml"
        }
    },
    "scripts": {
        "php_check": [
          "grumphp run --ansi"
        ]
    }
}
